
@{
    ViewBag.Title = "Treasury";
}


<section class="content-header content-toolbar">
    <div class="row">
        <div class="col-md-6">
            <div class="left-container">
                <div>
                    @(Html.DevExtreme().Button()
                        .Type(ButtonType.Normal)
                        .Icon("plus")
                        .Text("New")
                        .StylingMode(ButtonStylingMode.Contained)
                        .OnClick(@<text> function(){ window.location = "@Url.Action("TreasuryNew", "Fid")"; } </text>)
                        )
                </div>
            </div>
        </div>
        <div class="col-md-6">
            <div id="right-toolbar-container" class="right-container">

            </div>
        </div>
    </div>
</section>

<section class="content container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="box box-solid">
                <div class="box-body">
                    <div id="grid1"></div>
                </div>
            </div>
        </div>
    </div>
</section>


@section scripts
{
    <script src="~/Scripts/app/fid/treasury/index.js"></script>
}

