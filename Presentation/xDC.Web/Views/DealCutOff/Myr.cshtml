
@{
    ViewBag.Title = "Deal Cut Off";
    ViewBag.Subtitle = "MYR";
}

<section class="content container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="box box-solid">
                <div class="box-header">
                    <div class="left-container" style="display: inline-block">
                        <div style="display: inline-block" id="dateSelectionBtn"></div>
                        <div style="display: inline-block" id="viewTypeDropdown"></div>

                    </div>
                    <div class="right-container pull-right">

                        <div style="display: inline-block" id="formRemarksBtn"></div>
                        <div style="display: inline-block" id="printBtn"></div>
                    </div>
                </div>
                <div class="box-body">
                    <div id="tabpanel-container">
                        <div data-options="dxTemplate : { name: 'cashflowTab' }" style="padding-top: 10px; overflow: auto">
                            <div style="width: 100%">
                                <iframe id="sheet1" style="width: 100%; height: 1200px; border-style: none;"> </iframe>
                            </div>
                        </div>
                        <div data-options="dxTemplate : { name: 'moneyMarketTab' }" style="padding-top: 10px; overflow: auto">
                            <div style="width: 100%">
                                <iframe id="sheet2" style="width: 100%; height: 1200px; border-style: none;"> </iframe>
                            </div>
                        </div>
                        <div data-options="dxTemplate : { name: 'othersTab' }" style="padding-top: 10px; overflow: auto">
                            <div style="width: 100%">
                                <iframe id="sheet3" style="width: 100%; height: 1200px; border-style: none;"> </iframe>
                            </div>
                        </div>
                        <div data-options="dxTemplate : { name: 'auditListTab' }" style="padding-top: 10px; overflow: auto">
                            <div style="width: 100%">
                                <iframe id="sheet4" style="width: 100%; height: 1200px; border-style: none;"> </iframe>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

@Html.Partial("_FormsRemarksModal")


@section scripts
{
    <script src="~/Scripts/app/dealCutOff/myr.js"></script>
}