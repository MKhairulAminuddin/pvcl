@model dynamic

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "New Inflow Funds";
}


<section class="content-header content-toolbar">
    <div class="row">
        <div class="col-md-6">
            <div class="left-container">
            </div>
        </div>
        <div class="col-md-6">
            <div id="right-toolbar-container" class="right-container">
            </div>
        </div>
    </div>
</section>


<section class="content container-fluid">
    <div class="row">
        <div class="col-md-11">
            <form action="" id="amsd-form">
                <div class="row">
                    <div class="col-md-7">

                    </div>
                    <div class="col-md-5">
                        <div class="dx-fieldset" style="height: 30px;">
                            <div class="dx-field">
                                <div class="dx-field-label">Form ID: </div>
                                <div class="dx-field-value">
                                    <div id="tbFormId"></div>
                                </div>
                            </div>
                            <div class="dx-field">
                                <div class="dx-field-label">Form Status: </div>
                                <div class="dx-field-value">
                                    <div id="tbFormStatus"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-12">
                        <h4><span class="label label-success">INFLOW FUNDS</span></h4>
                        <div id="inflowFundsGrid1"></div>
                        <input type="hidden" name="inflowFundsInput"/>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-12">
                        <p style="float: right;margin-top: 10px">
                            <button class="btn btn-default" id="cancelBtn">Cancel</button>
                            <button class="btn btn-primary" type="submit">Submit</button>
                        </p>
                    </div>
                </div>
            </form>
        </div>
    </div>
</section>

@section scripts
{
    <script src="~/Scripts/app/amsd/newForm.js"></script>
}